@model IEnumerable<DoctorSkin.Models.Categories>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.namec)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.hide)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.date_up)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.meta)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.namec)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.hide)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.date_up)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.meta)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.typep }) |
                @Html.ActionLink("Details", "Details", new { id=item.typep }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.typep })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
